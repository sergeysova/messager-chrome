{
  "version": "2014.10.1.1",
  "name": "__MSG_CHROME_EXT_SHORT_NAME__",
  "description": "__MSG_CHROME_EXT_DESCRIPTION__",
  "default_locale": "ru",
  "manifest_version": 2,
   "minimum_chrome_version": "26.0.0.0",
  "externally_connectable": {
      "matches": [ "http://messager.lestad.net/*", "http://beta.messager.lestad.net/*" ]
   },
   "icons": {
     "128": "images/online_128.png",
      "48": "images/online_48.png"
   },
   "browser_action": {
      "default_icon": {
         "19": "images/online_20.png",
         "38": "images/online_48.png"
      },
      "default_title": "__MSG_CHROME_EXT_SHORT_NAME__"
   },
   "options_page": "settings.html",
   "permissions": [ "cookies", "background", "idle", "notifications", "tabs", "*://*.lestad.net/*" ],
   "web_accessible_resources": [ "*" ],
   "system_indicator": {
      "default_icon": {
         "19": "images/online_20.png",
         "38": "images/online_48.png"
      }
   },
   "background": {
      "scripts": [ "scripts/extension.js" ]
   }
}